{
	"compilerOptions": {
		"outDir": "./public/static/dist/",
		"sourceMap": true,
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "es5",
		"jsx": "react",
		"lib": ["es2015", "dom"],
		"skipLibCheck": true
	},
	"exclude": ["node_modules", "server", "target"]
}